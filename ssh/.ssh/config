Include ~/.orbstack/ssh/config

Host *
    ServerAliveInterval 240
    HostKeyAlgorithms +ssh-dss
    PubkeyAcceptedKeyTypes +ssh-rsa
    KexAlgorithms +diffie-hellman-group1-sha1,diffie-hellman-group14-sha1
    ControlMaster auto
    ControlPath ~/.ssh/session/%h-%p-%r
    ControlPersist 12h
Host 192.168.*.*
    CheckHostIP no
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
Host 172.20.*.*
    CheckHostIP no
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
Host *.local
    CheckHostIP no
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
Host chaney-*
    CheckHostIP no
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

Host chaney-dev
    HostName chaney-dev
    User chaney
    ForwardAgent yes
Host chaney-smtx
    HostName chaney-smtx
    User chaney
    ForwardAgent yes
Host chaney-xps13
    HostName chaney-xps13
    User chaney
    ForwardAgent yes
Host chaney-fedora
    HostName 192.168.28.220
    User chaney
    ForwardAgent yes
Host chaney-pi1
    HostName chaney-pi1
    User chaney
    ForwardAgent yes
Host chaney-pi2
    HostName chaney-pi2
    User chaney
    ForwardAgent yes
Host chaney-pi3
    HostName chaney-pi3
    User chaney
    ForwardAgent yes
