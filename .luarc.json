{
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "globals": ["vim", "hs", "Snacks"]
  },
  "workspace": {
    "checkThirdParty": false,
    "library": [
      "${env:VIMRUNTIME}",
      "${3rd}/luv/library",
      "${3rd}/busted/library"
    ]
  },
  "telemetry": {
    "enable": false
  }
}
